    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "APP_INSIGHT_KEY": "TO_BE_REPLACED",
                "APP_VERSION": "0.1.1",
                "AZURE_KEY_VAULT_URL": "TO_BE_REPLACED",
                "AZURE_CLIENT_ID": "TO_BE_REPLACED",
                "AZURE_TENANT_ID": "TO_BE_REPLACED",
                "SERVICE_BUS_KEY": "TO_BE_REPLACED",
                "AZURE_CLIENT_SECRET": "SET_IN_ENV"
            },
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}